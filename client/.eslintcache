\n              <li>\n                <Link onClick={handleDropdownClick} to={`/myprofile`}>\n                  <span>{renderPhoto()}</span>\n                  <span>profile</span>\n                </Link>\n              </li>\n\n              <li>\n                <Link onClick={handleDropdownClick} to=\"/settings\">\n                  <span>\n                    <FontAwesomeIcon icon={faCog} />\n                  </span>\n                  <span>settings</span>\n                </Link>\n              </li>\n              <li onClick={handleSignoutClick}>\n                <span>\n                  <FontAwesomeIcon icon={faSignOutAlt} />\n                </span>\n                <span>sign out</span>\n              </li>\n            </ul>\n          </div>\n          {/* </div> */}\n        </>\n      );\n    } else {\n      history.push(\"/register\");\n    }\n  };\n  return (\n    <nav>\n      <SideNav />\n\n      <div className=\"codependent-logo\">\n        <li>\n          <Link to=\"/\">\n            <p className=\"logo\">\n              co<span className=\"logo-emphasis\">[de]</span>pendent\n            </p>\n          </Link>\n        </li>\n      </div>\n      <div className=\"profile-nav\">\n        {userState && <FontAwesomeIcon icon={faUserCircle} />}\n        {userState && renderProfileContainer()}\n        {/* {renderProfileContainer()} */}\n      </div>\n      {/* <UserNav /> */}\n    </nav>\n  );\n}\n","/Users/homefolder/code/codependent/client/src/pages/Settings.js",["158","159","160","161","162"],"/Users/homefolder/code/codependent/client/src/pages/Profile.js",["163","164","165","166","167","168","169"],"/Users/homefolder/code/codependent/client/src/components/forms/AddComment.js",["170","171"],"/Users/homefolder/code/codependent/client/src/pages/PostPage.js",["172"],"/Users/homefolder/code/codependent/client/src/components/calendar/UpdateAvailability.js",["173","174"],"/Users/homefolder/code/codependent/client/src/pages/index.js",[],"/Users/homefo