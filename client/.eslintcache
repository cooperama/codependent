page-container\">\n        <div className=\"home-container\">\n          <div className=\"main-content\">\n            <div className=\"home-banner-div\">\n              <h1>co[de]pendent</h1>\n            </div>\n            <div className=\"home-post-containers\">\n              <div className=\"vertical-text\">\n                <p>Recent Forum Posts</p>\n                <FontAwesomeIcon icon={faChevronRight} />\n              </div>\n              <div className=\"recent-code-posts\">\n                {recentForumPosts && renderForumPosts()}\n              </div>\n            </div>\n            <div className=\"home-post-containers\">\n              <div className=\"vertical-text\">\n                <p>Recent Code Posts</p>\n                <FontAwesomeIcon icon={faChevronRight} />\n              </div>\n              <div className=\"recent-code-posts\">\n                {recentPosts && renderCodePosts()}\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n      {/* <div className=\"calendar-list\">\n        <div className=\"calendar-icon-div\">\n          <span>\n            <FontAwesomeIcon icon={faChevronRight} />\n          </span>\n          <span>\n            <FontAwesomeIcon icon={faCalendar} />\n          </span>\n        </div> */}\n      {/* Gonna wanna render calendar list for the week. (MyListView) */}\n      {/* </div> */}\n    </>\n  );\n}\n","/Users/homefolder/code/codependent/client/src/pages/NerdRoom.js",["103","104","105"],"/Users/homefolder/code/codependent/client/src/pages/UpdateAvail.js",["106","107"],"/Users/homefolder/code/codependent/client/src/components/calendar/UpdateAvailability.js",["108","109"],"/Users/homefolder/code/codependent/client/src/components/auth/Login.js",[],"/Users/homefolder/code/codependent/client/src/models/user.js",[],"/Users/homefolder/code/codependent/client/src/components/auth/Signup.js",["110","111"],"/Users/homefolder/code/codependent/client/src/pages/Profile.js",["112","